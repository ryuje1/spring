<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.site.dao.MemberMapper">
  <!-- 회원전체리스트 -->
  <select id="selectAll" resultType="com.site.dto.Member">
    select * from member
  </select>
  <!-- 회원정보 -->
  <select id="selectLogin" resultType="com.site.dto.Member">
    select * from member where id=#{param1} and pw=#{param2}
  </select>
  <!-- 
  <update id=""></update>
  <delete id=""></delete>
  <insert id=""></insert>
   -->
</mapper>